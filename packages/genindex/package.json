{
  "name": "@muds/genindex",
  "version": "0.2.0",
  "description": "Generational index library",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build && npm test && npm run doc",
    "clean": "rimraf coverage/ docs/ dist/",
    "prebuild": "npm run lint",
    "build": "npm run tsc && npm run babel",
    "lint": "eslint src --ext .ts",
    "babel": "babel src -d dist -x '.ts' --root-mode upward",
    "tsc": "tsc --project tsconfig.build.json",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "doc": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andykswong/muds.git"
  },
  "keywords": [
    "data-oriented",
    "genindex",
    "muds",
    "typescript"
  ],
  "author": "Andy K.S. Wong <andykswong@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andykswong/muds/issues"
  },
  "homepage": "https://github.com/andykswong/muds",
  "dependencies": {
  },
  "devDependencies": {
  }
}
