{
  "name": "@muds/react",
  "version": "0.1.0",
  "description": "muds React integration",
  "type": "module",
  "sideEffects": false,
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    "import": "./index.js"
  },
  "files": [
    "/dist",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "rimraf coverage docs dist node_modules **/__tests__/**/*.spec.map",
    "prebuild": "npm run lint",
    "build": "npm run babel && npm run tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "babel": "cross-env-shell babel src -d dist --source-maps --extensions '.ts,.tsx' --out-file-extension .js",
    "tsc": "tsc --emitDeclarationOnly",
    "test": "npm run jest",
    "jest": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
    "jest:watch": "npm run jest -- --watch",
    "doc": "typedoc --out docs src/index.ts",
    "examples": "webpack serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andykswong/muds.git"
  },
  "keywords": [
    "microframework",
    "muds",
    "react",
    "typescript"
  ],
  "author": "Andy K.S. Wong <andykswong@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andykswong/muds/issues"
  },
  "homepage": "https://github.com/andykswong/muds",
  "peerDependencies": {
    "@muds/runtime": ">=0.1",
    "react": ">=18.0",
    "react-dom": ">=18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18",
    "@babel/core": "^7.18",
    "@babel/preset-env": "^7.18",
    "@babel/preset-react": "^7.18",
    "@babel/preset-typescript": "^7.18",
    "@types/jest": "^29.0",
    "@types/node": "^18.7",
    "@types/react": "^18.0",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30",
    "@typescript-eslint/parser": "^5.30",
    "babel-jest": "^29.0",
    "babel-loader": "^8.2",
    "babel-plugin-add-import-extension": "^1.6",
    "copyfiles": "^2.4",
    "cross-env": "^7.0",
    "eslint": "^8.19",
    "html-webpack-plugin": "^5.5",
    "jest": "^29.0",
    "react": "^18.0",
    "react-dom": "^18.0",
    "rimraf": "^3.0",
    "source-map-loader": "^4.0.0",
    "typedoc": "^0.23",
    "typescript": "^4.7",
    "webpack": "^5.74",
    "webpack-cli": "^4.10",
    "webpack-dev-server": "^4.11"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.spec.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/__tests__/**"
    ],
    "coverageDirectory": "coverage"
  }
}
